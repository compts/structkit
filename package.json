{
  "name": "structkit",
  "version": "1.4.1",
  "description": "JS utility tool",
  "main": "node.cjs.js",
  "module": "dist/esm/node.es.js",
  "types": "dist/ts/node.esm.d.ts",
  "sideEffects": false,
  "bin": {
    "structkit": "./bin/structkit"
  },
  "scripts": {
    "es_linter": "eslint test/*/*.js src/function/*.js src/core/*.js *.js --fix",
    "test_esm": "mocha -r esm test/esm/*.test.js",
    "test_cjs": "mocha test/cjs/*.test.js",
    "validate": "npm run style && npm run test",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "tsd": {
    "directory": "test/ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compts/structkit.git"
  },
  "keywords": [
    "structkit"
  ],
  "author": "Pein Freccs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compts/structkit/issues"
  },
  "homepage": "https://github.com/compts/structkit#readme",
  "dependencies": {
    "tsd": "^0.19.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "eslint": "^7.16.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "mocha": "^5.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/compts-labs/",
      "/src_old_copy/",
      "/dist/",
      "/test/esm/"

    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
